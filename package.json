{
	"name": "openstack-tox",
	"displayName": "openstack-tox",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.65.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:openstack-tox.compile-docs",
		"onCommand:openstack-tox.init-tox-env",
		"onCommand:openstack-tox.debug-unittest",
		"onCommand:openstack-tox.run-test"
	],

	"main": "./out/extension.js",

	"contributes": {
		"commands": [
			{
				"command": "openstack-tox.compile-docs",
				"title": "OpenStack Tox: Compile Docs"
			},
			{
				"command": "openstack-tox.init-tox-env",
				"title": "OpenStack Tox: Init Tox Env"
			},
			{
				"command": "openstack-tox.debug-unittest",
				"title": "OpenStack Tox: Debug Unittest"
			},
			{
				"command": "openstack-tox.run-test",
				"title": "OpenStack Tox: Run Unittest"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "openstack-tox.debug-unittest",
					"when": "editorLangId == python"
				},
				{
					"command": "openstack-tox.run-test",
					"when": "editorLangId == python"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.2.0",
		"@types/mocha": "^9.1.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.65.0",
		"@typescript-eslint/eslint-plugin": "^5.16.0",
		"@typescript-eslint/parser": "^5.16.0",
		"@vscode/test-electron": "^2.1.3",
		"eslint": "^8.11.0",
		"glob": "^7.2.0",
		"mocha": "^9.2.2",
		"typescript": "^4.6.3"
	}
}
